#grid {
  width: 624px;
  height: 624px;
}

body {
  background: white;
}

.square {
  float: left;
  width: 200px;
  height: 200px;
  background: white;
}

.sq-1{
  border-right: 4px solid #C4C4B5;
  border-bottom: 4px solid #C4C4B5;
}

.sq-2{
  border-bottom: 4px solid #C4C4B5;
}

.sq-3{
  border-left: 4px solid #C4C4B5;
  border-bottom: 4px solid #C4C4B5;
}

.sq-4{
  border-right: 4px solid #C4C4B5;
  border-bottom: 4px solid #C4C4B5;
}

.sq-5{
  border-bottom: 4px solid #C4C4B5;
}

.sq-6{
  border-left: 4px solid #C4C4B5;
  border-bottom: 4px solid #C4C4B5;
}

.sq-7{
  border-right: 4px solid #C4C4B5;
}

.sq-8{
  border-right: 4px solid #C4C4B5;
}

.x {
  color: black;
  font-family: 'Permanent Marker';
  font-size: 500%;
}

.x > div {
  width:30%;
  height:50%;
  margin: 0 auto;
  -webkit-transform:rotate(9deg);
}

.o > div {
  width:30%;
  height:50%;
  margin: 0 auto;
  -webkit-transform:rotate(9deg);
}

.o {
  color: black;
  font-family: 'Permanent Marker';
  font-size: 500%;
}

.container{
  width:800px;
  margin: 0 auto;
}